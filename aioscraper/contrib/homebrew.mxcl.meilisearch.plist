<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>KeepAlive</key>
    <false/>
    <key>Label</key>
    <string>homebrew.mxcl.meilisearch</string>
    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/opt/meilisearch/bin/meilisearch</string>
      <string>--db-path</string>
      <string>/usr/local/var/meilisearch/data.ms</string>
      <string>--http-addr</string>
      <string>127.0.0.1:7701</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>WorkingDirectory</key>
    <string>/usr/local/var</string>
    <key>StandardErrorPath</key>
    <string>/usr/local/var/log/meilisearch.log</string>
    <key>StandardOutPath</key>
    <string>/usr/local/var/log/meilisearch.log</string>
  </dict>
</plist>
